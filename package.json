{
  "name": "payment-processor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/devServer.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "tsc": "tsc",
    "dev": "nodemon --delay 2 --watch src -e ts,ejs --exec npm run dev:start",
    "remove": "eval $(aws configure export-credentials --profile ${AWS_PROFILE} --format env) && env -u AWS_PROFILE sls remove --stage",
    "deploy": "eval $(aws configure export-credentials --profile ${AWS_PROFILE} --format env) && env -u AWS_PROFILE sls deploy --stage",
    "start": "node .",
    "test": "npx jest --config jest-unit.config.ts",
    "test:integration:dev": "DOTENV_CONFIG_PATH=.env.dev npx jest ./src/test/integration/",
    "test:integration": "npx jest ./src/test/integration/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/axios": "^0.14.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^11.0.0",
    "esbuild": "^0.25.10",
    "express": "^5.1.0",
    "jest": "^30.1.3",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "serverless-domain-manager": "^8.0.0",
    "serverless-esbuild": "^1.55.1",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "fast-xml-parser": "^5.2.5",
    "joi": "^18.0.1",
    "node-fetch": "^3.3.2"
  }
}
